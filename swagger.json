    "paths": {
      "/giaoduc/{id}": {
        "get": {
          "tags": [
            "Giáo dục"
          ],
          "summary": "Find article by ID",
          "description": "Returns a single article",
          "operationId": "getArticleById",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID of article to return ( 1 - 30 )",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int64"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Giaoduc"
                  }
                }          
              }
            },
            "400": {
              "description": "Invalid ID supplied"
            },
            "404": {
              "description": "Article not found"
            }
          }
        }
      },
      "/tretho/{id}": {
        "get": {
          "tags": [
            "Trẻ thơ"
          ],
          "summary": "Find article by ID",
          "description": "Returns a single article",
          "operationId": "getArticleById",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID of article to return ( 1 - 20 )",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int64"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/Tretho"
                  }
                }          
              }
            },
            "400": {
              "description": "Invalid ID supplied"
            },
            "404": {
              "description": "Article not found"
            }
          }
        }
      },
      "/findByType": {
        "get": {
          "tags": [
            "Trẻ thơ"
          ],
          "summary": "Find article by Type",
          "description": "Returns multiple  article",
          "operationId": "findArticlesByType",
          "parameters": [
            {
              "name": "type",
              "in": "query",
              "description": "",
              "required": false,
              "explode": true,
              "schema": {
                "type": "string",
                "default": "available",
                "enum": [
                  "Ngẫm chuyện đời",
                  "Trải nghiệm cuộc sống",
                  "Bí kíp du lịch",
                  "Du lịch nước ngoài"
                ]
              }
            }
          ],
          "responses": {
            "200": {
              "description": "successful operation",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/Tretho"
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid status value"
            }
          }
          }
        } 
    },
    
    "components": {
      "schemas": {
